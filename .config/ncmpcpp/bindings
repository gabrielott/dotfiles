# Movement
def_key "h"
  previous_column

def_key "h"
  jump_to_parent_directory

def_key "h"
  master_screen

def_key "j"
  scroll_down

def_key "k"
  scroll_up

def_key "l"
  next_column

def_key "l"
  enter_directory

def_key "l"
  play_item

def_key "l"
  slave_screen

def_key "ctrl-u"
  page_up

def_key "ctrl-d"
  page_down

def_key "g"
  move_home

def_key "G"
  move_end

def_key "["
  scroll_up_album

def_key "]"
  scroll_down_album

def_key "{"
  scroll_up_artist

def_key "}"
  scroll_down_artist

def_key "space"
  select_item
  scroll_down

def_key "enter"
  run_action

def_key "/"
  find

def_key "/"
  find_item_forward

def_key "?"
  find

def_key "?"
  find_item_backward

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

# Browser
